{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "yuuvis-flokfugl": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "prefix": "yuv",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/yuuvis-flokfugl",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.app.json",
            "assets": ["src/favicon.ico", "src/assets", "src/manifest.json"],
            "styles": ["src/styles.scss"],
            "scripts": [],
            "es5BrowserSupport": true
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ],
              "serviceWorker": true,
              "ngswConfigPath": "ngsw-config.json"
            },
            "dist": {
              "tsConfig": "src/tsconfig.dist.json"
            },
            "dev": {
              "tsConfig": "src/tsconfig.dev.json"
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "yuuvis-flokfugl:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "yuuvis-flokfugl:build:production"
            },
            "dist": {
              "browserTarget": "yuuvis-flokfugl:build:dist"
            },
            "dev": {
              "browserTarget": "yuuvis-flokfugl:build:dev"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "yuuvis-flokfugl:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "karmaConfig": "src/karma.conf.js",
            "styles": ["src/styles.scss"],
            "scripts": [],
            "assets": ["src/favicon.ico", "src/assets", "src/manifest.json"]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["src/tsconfig.app.json", "src/tsconfig.spec.json"],
            "exclude": ["**/node_modules/**"]
          }
        }
      }
    },
    "yuuvis-flokfugl-e2e": {
      "root": "e2e/",
      "projectType": "application",
      "prefix": "",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "yuuvis-flokfugl:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "yuuvis-flokfugl:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "e2e/tsconfig.e2e.json",
            "exclude": ["**/node_modules/**"]
          }
        }
      }
    },
    "@yuuvis/common-ui": {
      "root": "projects/yuuvis/common-ui",
      "sourceRoot": "projects/yuuvis/common-ui/src",
      "projectType": "library",
      "prefix": "yuv",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/yuuvis/common-ui/tsconfig.lib.json",
            "project": "projects/yuuvis/common-ui/ng-package.json"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/yuuvis/common-ui/src/test.ts",
            "tsConfig": "projects/yuuvis/common-ui/tsconfig.spec.json",
            "karmaConfig": "projects/yuuvis/common-ui/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["projects/yuuvis/common-ui/tsconfig.lib.json", "projects/yuuvis/common-ui/tsconfig.spec.json"],
            "exclude": ["**/node_modules/**"]
          }
        }
      }
    },
    "@yuuvis/core": {
      "root": "projects/yuuvis/core",
      "sourceRoot": "projects/yuuvis/core/src",
      "projectType": "library",
      "prefix": "yuv",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/yuuvis/core/tsconfig.lib.json",
            "project": "projects/yuuvis/core/ng-package.json"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/yuuvis/core/src/test.ts",
            "tsConfig": "projects/yuuvis/core/tsconfig.spec.json",
            "karmaConfig": "projects/yuuvis/core/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["projects/yuuvis/core/tsconfig.lib.json", "projects/yuuvis/core/tsconfig.spec.json"],
            "exclude": ["**/node_modules/**"]
          }
        }
      }
    },
    "@yuuvis/framework": {
      "root": "projects/yuuvis/framework",
      "sourceRoot": "projects/yuuvis/framework/src",
      "projectType": "library",
      "prefix": "yuv",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/yuuvis/framework/tsconfig.lib.json",
            "project": "projects/yuuvis/framework/ng-package.json"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/yuuvis/framework/src/test.ts",
            "tsConfig": "projects/yuuvis/framework/tsconfig.spec.json",
            "karmaConfig": "projects/yuuvis/framework/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["projects/yuuvis/framework/tsconfig.lib.json", "projects/yuuvis/framework/tsconfig.spec.json"],
            "exclude": ["**/node_modules/**"]
          }
        }
      }
    },
    "component-tests": {
      "root": "",
      "sourceRoot": "src-component-tests",
      "projectType": "application",
      "prefix": "app",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/component-tests",
            "index": "src-component-tests/index.html",
            "main": "src-component-tests/main.ts",
            "polyfills": "src-component-tests/polyfills.ts",
            "tsConfig": "src-component-tests/tsconfig.test.json",
            "assets": ["src-component-tests/favicon.ico", "src-component-tests/assets"],
            "styles": ["src-component-tests/styles.scss"],
            "scripts": [],
            "es5BrowserSupport": true
          },
          "configurations": {
            "test": {
              "fileReplacements": [
                {
                  "replace": "src-component-tests/environments/environment.ts",
                  "with": "src-component-tests/environments/environment.prod.ts"
                },
                {
                  "replace": "src-component-tests/assets/default/i18n/en.json",
                  "with": "src/assets/default/i18n/en.json"
                },
                {
                  "replace": "src-component-tests/assets/default/i18n/de.json",
                  "with": "src/assets/default/i18n/de.json"
                },
                {
                  "replace": "src-component-tests/assets/default/config/main.json",
                  "with": "src/assets/default/config/main.json"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "component-tests:build"
          },
          "configurations": {
            "test": {
              "browserTarget": "component-tests:build:test"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "component-tests:build"
          }
        }
      }
    }
  },
  "defaultProject": "yuuvis-flokfugl"
}
