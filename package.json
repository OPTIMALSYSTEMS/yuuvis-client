{
  "name": "yuuvis-flokfugl",
  "version": "1.0.0-beta.1",
  "releaseDate": "1.0",
  "description": "yuuvisÂ® API client",
  "author": "OPTIMAL SYSTEMS GmbH",
  "scripts": {
    "ng": "ng",
    "prebuild:about": "node ./tools/scripts/common/about/prebuild-about-state",
    "start": "ng serve --port 4400 --proxy-config proxy.config.js",
    "start:dist": "npm run start -- --configuration dist",
    "start:dev": "npm run start -- --configuration dev",
    "start:cmp:test": "ng serve component-tests --configuration test --port 4500 --proxy-config proxy.config.js",
    "build:cmp:test": "ng build component-tests --configuration test",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "test:e2e": "cd e2e/cypress && npm run test:e2e",
    "test:e2e:open": "cd e2e/cypress && npm run test:e2e:open",
    "test:comp": "cd e2e/cypress && npm run test:comp",
    "test:comp:open": "cd e2e/cypress && npm run test:comp:open",
    "cleanup:dist": "rimraf dist",
    "doc:framework": "cd projects/yuuvis/framework && npm run doc",
    "doc:framework:serve": "cd projects/yuuvis/framework && npm run doc:serve",
    "doc:common": "cd projects/yuuvis/common-ui && npm run doc",
    "doc:common:serve": "cd projects/yuuvis/common-ui && npm run doc:serve",
    "doc:core": "cd projects/yuuvis/core && npm run doc",
    "doc:core:serve": "cd projects/yuuvis/core && npm run doc:serve",
    "style:libs": "node ./tools/scripts/common/sass --lib=common-ui && node ./tools/scripts/lib/framework-postbuild",
    "build:core": "ng build @yuuvis/core && node ./tools/scripts/common/node-copy --lib=core",
    "build:common": "ng build @yuuvis/common-ui && node ./tools/scripts/common/sass --lib=common-ui && node ./tools/scripts/common/node-copy --lib=common-ui",
    "build:framework": "node ./tools/scripts/lib/framework-prebuild && ng build @yuuvis/framework && node ./tools/scripts/lib/framework-postbuild",
    "extract:framework": "ngx-translate-extract --input ./projects/yuuvis/framework/src --output ./projects/yuuvis/framework/src/assets/i18n/de.json ./projects/yuuvis/framework/src/assets/i18n/en.json --clean --sort --format json",
    "extract:app": "ngx-translate-extract --input ./src --output ./src/assets/default/i18n/de.json ./src/assets/default/i18n/en.json --clean --sort --format json && node ./tools/scripts/common/i18n-merge --run",
    "extract:merge:all": "npm run extract:framework && node ./tools/scripts/common/i18n --lib=framework && npm run extract:app",
    "sass:libs": "node ./tools/scripts/common/sass --lib=common-ui&& node ./tools/scripts/common/sass --lib=framework",
    "build:libs:all": "npm run build:core && npm run build:common && npm run build:framework",
    "build:cordova": "ng build cordova --baseHref=./ --outputPath=cordova/www",
    "build:android": "npm run build:cordova && cd cordova && cordova clean android && cordova build android",
    "build:android:run": "npm run build:cordova && cd cordova && cordova clean android && cordova run android",
    "build:web": "npm run prebuild:about && npm run build:libs:all && npm run build -- --base-href /client/"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.0.0",
    "@angular/cdk": "^7.3.7",
    "@angular/common": "~8.0.0",
    "@angular/compiler": "~8.0.0",
    "@angular/core": "~8.0.0",
    "@angular/forms": "~8.0.0",
    "@angular/platform-browser": "~8.0.0",
    "@angular/platform-browser-dynamic": "~8.0.0",
    "@angular/router": "~8.0.0",
    "@ionic-native/core": "^5.6.0",
    "@ionic-native/in-app-browser": "^5.6.0",
    "@ngx-pwa/local-storage": "^7.4.2",
    "@ngx-translate/core": "^11.0.1",
    "ag-grid-angular": "^21.0.1",
    "ag-grid-community": "^21.0.1",
    "angular-resize-event": "^1.0.0",
    "angular-split": "^3.0.1",
    "core-js": "^2.5.4",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "primeicons": "^2.0.0",
    "primeng": "^8.0.2",
    "rxjs": "~6.5.2",
    "tslib": "^1.9.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.800.0",
    "@angular-devkit/build-ng-packagr": "~0.800.0",
    "@angular/cli": "~8.0.1",
    "@angular/compiler-cli": "~8.0.0",
    "@angular/language-service": "~8.0.0",
    "@compodoc/compodoc": "^1.1.9",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "bundle-scss": "^1.4.6",
    "codelyzer": "^5.0.1",
    "cpx": "^1.5.0",
    "fs-extra": "^7.0.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^5.3.0",
    "ngx-translate-extract": "^1.0.0",
    "node-dir": "^0.1.17",
    "node-sass": "^4.12.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tsickle": ">=0.34.0",
    "tslib": "^1.9.0",
    "tslint": "~5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~3.4.5"
  }
}
