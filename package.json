{
  "name": "yuuvis-flokfugl",
  "version": "1.0.0-beta.1",
  "releaseDate": "2.0",
  "description": "yuuvisÂ® flex client",
  "author": "OPTIMAL SYSTEMS GmbH",
  "scripts": {
    "ng": "ng",
    "prebuild:about": "node ./tools/scripts/common/about/prebuild-about-state",
    "start": "ng serve --port 4400 --proxy-config proxy.config.js",
    "start:dist": "npm run start -- --configuration dist",
    "start:dev": "npm run start -- --configuration dev",
    "start:cmp:test": "ng serve component-tests --configuration test --port 4500 --proxy-config proxy.config.js",
    "start:tenant": "ng serve tenant-login --configuration dev --port 4600",
    "build:cmp:test": "ng build component-tests --configuration test",
    "start:docs": "ng serve documentation --configuration dev --port 4600 --proxy-config proxy.config.js",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "test:e2e": "cd e2e/cypress && npm run test:e2e",
    "test:e2e:open": "cd e2e/cypress && npm run test:e2e:open",
    "test:install": "cd e2e/cypress && npm install",
    "test:comp": "cd e2e/cypress && npm run test:comp",
    "test:comp:open": "cd e2e/cypress && npm run test:comp:open",
    "test:report": "cd e2e/cypress && npm run report",
    "cleanup:dist": "rimraf dist",
    "doc:framework": "npm run build:framework && cd projects/yuuvis/framework && npm run doc",
    "doc:common": "npm run build:common && cd projects/yuuvis/common-ui && npm run doc",
    "doc:core": "npm run build:core && cd projects/yuuvis/core && npm run doc",
    "doc:all": "npm run doc:core && npm run doc:common && npm run doc:framework",
    "style:libs": "node ./tools/scripts/common/sass --lib=common-ui && node ./tools/scripts/common/node-copy --lib=common-ui && node ./tools/scripts/lib/framework-postbuild",
    "build:core": "ng build @yuuvis/core && node ./tools/scripts/common/node-copy --lib=core",
    "build:common": "ng build @yuuvis/common-ui && node ./tools/scripts/common/sass --lib=common-ui && node ./tools/scripts/common/node-copy --lib=common-ui",
    "build:framework": "node ./tools/scripts/lib/framework-prebuild && ng build @yuuvis/framework && node ./tools/scripts/lib/framework-postbuild",
    "extract:framework": "ngx-translate-extract --input ./projects/yuuvis/framework/src --output ./projects/yuuvis/framework/src/assets/i18n/de.json ./projects/yuuvis/framework/src/assets/i18n/en.json --clean --sort --format json --marker marker",
    "extract:app": "ngx-translate-extract --input ./src --output ./src/assets/default/i18n/de.json ./src/assets/default/i18n/en.json --clean --sort --format json --marker marker",
    "extract:merge:all": "npm run extract:framework && node ./tools/scripts/common/i18n --lib=framework && npm run extract:app && node ./tools/scripts/common/i18n-merge --run",
    "sass:libs": "node ./tools/scripts/common/sass --lib=common-ui&& node ./tools/scripts/common/sass --lib=framework",
    "build:libs:all": "npm run build:core && npm run build:common && npm run build:framework",
    "build:web": "npm run prebuild:about && npm run build:libs:all && npm run build -- --prod --base-href /client/ && node ./tools/scripts/misc/replace-start-url --url=/client/ && npm run prepare:dist",
    "build:pwa": "npm run prebuild:about && npm run build:libs:all && npm run build -- --prod --base-href /clientdev/ && node ./tools/scripts/misc/replace-start-url --url=/clientdev/ && npm run prepare:dist",
    "build:pwa:loc": "npm run prebuild:about && npm run build:libs:all && npm run build -- --prod --base-href /yuuvis-flokfugl/ && node ./tools/scripts/misc/replace-start-url --url=/yuuvis-flokfugl/ && npm run prepare:dist",
    "prepare:dist": "node ./tools/scripts/misc/prepare-dist",
    "serve:dist": "node ./tools/scripts/dist-serve",
    "docker:create:tag": "node ./tools/scripts/misc/create-random-tag",
    "docker:create:image": "npm run docker:create:tag && cd docker && create-image.bat",
    "k8s:prepare": "node ./tools/scripts/misc/copy-k8s-descriptors",
    "k8s:deploy": "cd dist/k8s && kubectl apply -f namespace.yaml -f deployment.yaml -f service.yaml",
    "deploy:dist": "npm run docker:create:image && npm run k8s:prepare && npm run k8s:deploy",
    "deploy:dev": "npm run build:pwa && npm run docker:create:image && npm run k8s:prepare && npm run k8s:deploy",
    "i18n:diff": "node ./tools/scripts/common/i18n-diff.js"
  },
  "private": true,
  "dependencies": {
    "@ag-grid-community/angular": "^22.0.0",
    "@ag-grid-community/client-side-row-model": "^22.0.0",
    "@ag-grid-community/core": "^22.0.0",
    "@angular/animations": "~8.0.0",
    "@angular/cdk": "^7.3.7",
    "@angular/common": "~8.0.0",
    "@angular/compiler": "~8.0.0",
    "@angular/core": "~8.0.0",
    "@angular/forms": "~8.0.0",
    "@angular/platform-browser": "~8.0.0",
    "@angular/platform-browser-dynamic": "~8.0.0",
    "@angular/pwa": "^0.803.4",
    "@angular/router": "~8.0.0",
    "@angular/service-worker": "^8.0.0",
    "@biesbjerg/ngx-translate-extract-marker": "^1.0.0",
    "@ngx-pwa/local-storage": "^7.4.2",
    "@ngx-translate/core": "^11.0.1",
    "angular-resize-event": "^1.0.0",
    "angular-split": "^3.0.1",
    "core-js": "^2.5.4",
    "lodash-es": "^4.17.15",
    "ngx-device-detector": "^1.3.19",
    "ngx-drag-scroll": "^8.0.0-beta.2",
    "ngx-toastr": "^11.1.0",
    "primeicons": "^2.0.0",
    "primeng": "^8.0.2",
    "rxjs": "~6.5.2",
    "take-until-destroy": "^2.1.0",
    "tslib": "^1.9.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.800.0",
    "@angular-devkit/build-ng-packagr": "~0.800.0",
    "@angular/cli": "~8.0.1",
    "@angular/compiler-cli": "~8.0.0",
    "@angular/language-service": "~8.0.0",
    "@compodoc/compodoc": "^1.1.5",
    "@types/faker": "^4.1.8",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "bundle-scss": "^1.4.6",
    "codelyzer": "^5.0.1",
    "cpx": "^1.5.0",
    "express": "^4.17.1",
    "faker": "^4.1.0",
    "fs-extra": "^7.0.1",
    "http-proxy-middleware": "^0.20.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^5.3.0",
    "ngx-translate-extract": "^1.0.0",
    "node-dir": "^0.1.17",
    "node-sass": "^4.13.0",
    "protractor": "~5.4.0",
    "randomhex": "^0.1.5",
    "replace-in-file": "^4.1.3",
    "ts-node": "~7.0.0",
    "tsickle": ">=0.34.0",
    "tslib": "^1.9.0",
    "tslint": "~5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~3.4.5",
    "yargs": "^15.0.2"
  }
}
