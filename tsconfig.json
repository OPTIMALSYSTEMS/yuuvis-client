{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "es2015",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "@yuuvis/common-ui": [
        "dist/yuuvis/common-ui"
      ],
      "@yuuvis/common-ui/*": [
        "dist/yuuvis/common-ui/*"
      ],
      "@yuuvis/core": [
        "dist/yuuvis/core"
      ],
      "@yuuvis/core/*": [
        "dist/yuuvis/core/*"
      ],
      "@yuuvis/framework": [
        "dist/yuuvis/framework"
      ],
      "@yuuvis/framework/*": [
        "dist/yuuvis/framework/*"
      ]
    }
  }
}